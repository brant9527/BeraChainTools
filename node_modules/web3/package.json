    "test:blackbox:geth:ws": "./scripts/black_box_test.sh geth ws",
    "test:blackbox:hardhat:http": "./scripts/black_box_test.sh hardhat http",
    "test:blackbox:hardhat:ws": "./scripts/black_box_test.sh hardhat ws",
    "test:blackbox:infura:http": "./scripts/black_box_test.sh infura http",
    "test:blackbox:infura:ws": "./scripts/black_box_test.sh infura ws",
    "test:ci": "jest --coverage=true --coverage-reporters=json --verbose",
    "test:coverage:integration": "jest --config=./test/integration/jest.config.js --forceExit --coverage=true --coverage-reporters=text",
    "test:coverage:unit": "jest --config=./test/unit/jest.config.js --coverage=true --coverage-reporters=text",
    "test:e2e:chrome:stress": "npx cypress run --headless --browser chrome",
    "test:e2e:mainnet": "jest --config=./test/e2e/jest.config.js --forceExit --runInBand",
    "test:e2e:sepolia": "jest --config=./test/e2e/jest.config.js --forceExit --runInBand",
    "test:integration": "jest --config=./test/integration/jest.config.js --forceExit",
    "test:integration:stress": "jest --config=./test/stress/jest.config.js --forceExit",
    "test:sync:integration": "jest --config=./test/integration/jest.config.js ./test/integration/sync.test.ts",
    "test:unit": "jest --config=./test/unit/jest.config.js",
    "test:watch": "npm test -- --watch",
